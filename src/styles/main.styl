 *
 -moz-box-sizing: border-box
 -webkit-box-sizing: border-box
 box-sizing: border-box
 margin: 0
 padding: 0
 -webkit-touch-callout: none
 -webkit-user-select: none
 -khtml-user-select: none
 -moz-user-select: none
 -ms-user-select: none
 user-select: none



html, body
  line-height: 1
  font: 16px/1.5 
  fot-family: Helvetica Neue,Helvetica,PingFang SC,Hiragino Sans GB,Microsoft YaHei,SimSun,sans-serif
ul, li
    list-style: none
.clear
  clear: both
  &:after, &:before
    content: " "
    display: table
    clear: both

