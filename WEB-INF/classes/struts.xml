<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <constant name="struts.enable.DynamicMethodInvocation" value="true" />

    <package name="default" namespace="/" extends="struts-default">
        <action name="FileInFolderAction" class="ZxCloud.web.struts.action.FileInFolderAction">
        	<result>/index.html</result>
        </action>
        <action name="FileUploadAction" class="ZxCloud.web.struts.action.FileUploadAction" method="ajaxAttachUpload">
        	<result>/index.html</result>
        </action>
        <action name="FolderAction" class="ZxCloud.web.struts.action.FolderAction">
        	<result>/index.html</result>
        </action>
        <action name="GarbageAction" class="ZxCloud.web.struts.action.GarbageAction">
        	<result>/index.html</result>
        </action>
        <action name="UserAction" class="ZxCloud.web.struts.action.UserAction">
        	<result>/index.html</result>
        </action>
        
    </package>

</struts>