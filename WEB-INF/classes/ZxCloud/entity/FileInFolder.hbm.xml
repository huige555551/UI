<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  

<hibernate-mapping>
	<class name="ZxCloud.entity.FileInFolder" table="fileInFolder">
		<id name="id" column="id">
			<generator class="assigned"/>
		</id>
		<property name="name"/>
		<many-to-one name="folder" class="ZxCloud.entity.Folder" column="folderId"/>
		<many-to-one name="file" class="ZxCloud.entity.ZxFile" column="fileId"/>
		<property name="testProperty"/>
		<property name="state"/>
		<property name="createTime"/>
		
	</class>	
</hibernate-mapping>