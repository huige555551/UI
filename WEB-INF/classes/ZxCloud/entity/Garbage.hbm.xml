<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  

<hibernate-mapping>
	<class name="ZxCloud.entity.Garbage" table="garbage">
		<id name="id" column="id">
			<generator class="assigned"/>
		</id>
		<property name="putIntoTime"/>
		<property name="createTime"/>
		<many-to-one name="folder" unique="true">
			<column name="folder_id"/>
		</many-to-one>
		<many-to-one name="file" unique="true">
			<column name="file_id"/>
		</many-to-one>
		<many-to-one name="user" class="ZxCloud.entity.User" column="user_id"/>
	</class>	
</hibernate-mapping>